/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=c);
    And(a=b, b=c, out=d);
    And(a=a, b=sel, out=e);
    Or(a=d, b=e, out=out);
}